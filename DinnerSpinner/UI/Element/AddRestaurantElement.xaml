<UserControl x:Class="DinnerSpinner.UI.Element.AddRestaurantElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DinnerSpinner.UI.Element"
             xmlns:vm="clr-namespace:DinnerSpinner.UI.ViewModel"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=vm:AddRestaurantViewModel, IsDesignTimeCreatable=False}"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <TextBox Text="{Binding Name}"/>
        <Button Content="{StaticResource ResourceKey=PlusIcon}"/>
    </StackPanel>
</UserControl>
